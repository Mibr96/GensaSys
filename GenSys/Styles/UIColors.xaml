<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#0A003A</Color>
    <Color x:Key="primaryBackColor2">#1C0E49</Color>
    <Color x:Key="secondaryBackColor1">#24105F</Color>
    <Color x:Key="secondaryBackColor2">#210F55</Color>
    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">WhiteSmoke</Color>
    <Color x:Key="winBorderColor2">WhiteSmoke</Color>
    <Color x:Key="winBorderColor3">WhiteSmoke</Color>
    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="color2" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="color3" Color="#4ADAEC"/>
    <SolidColorBrush x:Key="color4" Color="#FB539B"/>
    <SolidColorBrush x:Key="color5" Color="#00FEFB"/>
    <SolidColorBrush x:Key="color6" Color="#00FEFB"/>
    <SolidColorBrush x:Key="color7" Color="#EF6C96"/>
    <SolidColorBrush x:Key="color8" Color="#00FEFB"/>
    <SolidColorBrush x:Key="color9" Color="#07F3C0"/>
    <SolidColorBrush x:Key="color10" Color="#FBA1AA"/>
    <!--Font Colors-->
    <SolidColorBrush x:Key="titleColor1" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="titleColor2" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="titleColor3" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="WhiteSmoke"/>
    <!--Panel Colors-->
    <SolidColorBrush x:Key="panelColor" Color="#200F53"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#1786F9"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#1786F9"/>
    

</ResourceDictionary>